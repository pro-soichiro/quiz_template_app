<h2>ログイン</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "staffs/shared/error_messages", resource: resource %>

  <table>
    <tr>
      <th>Employ Number</th>
      <td colspan="2"><%= f.text_field :employ_number,:placeholder =>"000" %></td>
    </tr>
    <tr>
      <th>パスワード(6文字以上)</th>
      <td colspan="2"><%= f.password_field :password %></td>
    </tr>
  </table>

  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
<% end %>

<%= render "staffs/shared/links" %>
