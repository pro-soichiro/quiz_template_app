<table>
  <thead>
    <tr>
      <th></th>
      <th>名前</th>
      <th>達成率</th>
      <th>正答率</th>
      <th>最終ログイン日時</th>
      <th>スタッフステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @staffs.each do |staff| %>
      <tr>
        <td><%= staff.id %></td>
        <td><%= link_to staff.full_name,admin_staff_path(staff) %></td>
        <td>達成率</td>
        <td>正答率</td>
        <td><%= staff.current_sign_in_at.strftime("%Y/%m/%d %H:%M:%S") %></td>
        <td>
          <!--実装途中-->
  				<% if staff.status == true && staff_signed_in? == true %>
    				<span class="text-login">ログイン中</span>
  				<% elsif staff.status == true && staff_signed_in? == false %>
    				<span class="text-logout">ログアウト</span>
    			<% else %>
  					<span class="text-invalid">退職</span>
  				<% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>